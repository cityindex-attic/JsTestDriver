<idea-plugin>
  <name>JSTestDriver Plugin</name>
  <description>Execute JavaScript unit tests using JSTestDriver</description>
  <version>0.1</version>
  <vendor>Google</vendor>
  <idea-version since-build="7973"/>
  <depends>com.intellij.modules.lang</depends>
  <application-components>
    <component>
      <implementation-class>com.google.jstestdriver.idea.JSTestDriverComponent</implementation-class>
    </component>
  </application-components>
  <project-components>
    <component>
      <implementation-class>com.google.jstestdriver.idea.JSTestDriverToolWindow</implementation-class>
    </component>
  </project-components>
</idea-plugin>